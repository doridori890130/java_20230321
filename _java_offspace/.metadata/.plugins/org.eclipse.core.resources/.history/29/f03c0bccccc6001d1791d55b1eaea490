package day17;

import java.io.File;

public class FileObjcectEX01 {

	public static void main(String[] args) {
		/*	파일이 가지고 있는 정보를 출력
		 * 	D:\_java_offspace\java_workspace
		 * */
		
		File f = new File("D:\\_java_offspace\\java_workspace\\out.txt");
		String fileName = f.getName();
		System.out.println(fileName);

	}

}
